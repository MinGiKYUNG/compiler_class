int add(int a, int b) {
    return a + b;
}

int sub(int a, int b) {
    return a - b;
}

int mul(int a, int b) {
    return a * b;
}

int div(int a, int b) {
    return a / b;
}

int main() {
    int x;
    int y;
    int result;
    
    x = 10;
    y = 3;
    
    result = add(x, y);
    result = sub(result, mul(x, y));
    result = div(result, add(x, y));
    
    return result;
}